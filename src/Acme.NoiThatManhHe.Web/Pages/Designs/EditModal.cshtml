@page
@model Acme.NoiThatManhHe.Web.Pages.Designs.EditModalModel
@using Acme.NoiThatManhHe.Web.Pages.Designs
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@{
    Layout = null;  
}
<abp-dynamic-form abp-model="Design" asp-page="/Designs/EditModal">
    <abp-modal>
        <abp-modal-header title="Update"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>

